{
  "web": {
    "client_id": "{CLIENT_ID}",
    "client_secret": "{CLIENT_SECRET}",
    "auth_uri": "{DashBoardURI}/oauth2/default/v1/authorize",
    "token_uri": "{DashBoardURI}/oauth2/default/v1/token",
    "issuer": "{DashBoardURI}/oauth2/default",
    "userinfo_uri": "{DashBoardURI}/oauth2/default/userinfo",
    "redirect_uris": [
      "http://localhost:5000/oidc/callback",
      "http://127.0.0.1:5000/oidc/callback"
    ]
  }
}
